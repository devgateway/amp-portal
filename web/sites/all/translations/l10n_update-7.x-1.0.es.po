msgstr "Módulos"
msgid "Available updates"
msgstr "Actualizaciones disponibles"
msgid "Up to date"
msgstr "Actualizado"
msgid "Update available"
msgstr "Actualización disponible"
msgid "Languages"
msgstr "Idiomas"
msgid "warning"
msgstr "advertencia"
msgid "Themes"
msgstr "Temas"
msgid "Multilingual"
msgstr "Multi idioma"
